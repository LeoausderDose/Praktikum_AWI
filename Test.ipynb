    "import xarray as xr\n",
    "\n",
    "# Create a sample DataArray\n",
    "data = xr.DataArray(\n",
    "    [[10, 20, 30],\n",
    "     [40, 50, 60],\n",
    "     [70, 80, 90]],\n",
    "    dims=(\"x\", \"y\")\n",
    ")\n",
    "print(data)\n",
    "# Select values along the 'x' dimension between 5 and 90\n",
    "selected_data = data.sel(y=slice(2, 91))\n",
    "print(selected_data)"
   ]
  }
